<h1>Sign in</h1>
<%# not using model, since i am not accesing any table here, instead just going to perform POST	/session(.:format)%>
<%= form_with url: session_path, local: true do |f|  %>
<div>
    <%= f.label :email %>
    <%= f.email_field :email%>
</div>
<div>
    <%= f.label :password  %> 
    <%= f.password_field :password  %> 
</div>
<%= f.submit "Sign In"  %> 
<% end %> 